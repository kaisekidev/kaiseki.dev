    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-highlight-words": "^0.16.7",
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.0",
